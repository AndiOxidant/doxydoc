html
    head
        title $title
        meta charset="UTF-8"
        script type="text/javascript" src="${basePath}doxydoc.js"
        :each $customJS
            script type="text/javascript" src="$this"

        link rel="Stylesheet" href="${basePath}main.css"
        link rel="stylesheet" href="${basePath}highlight.css"
        :each $customCSS
            link rel="Stylesheet" href="$this"
    body
        section class="pageMain"
            (> header)
            section class="pageBody"
                aside class="pageLeft"
                    :each $navigationLinks : div class="pageLinks"
                        span class="link"
                            a href="$link" target="$target" $name
                section class="pageContent"
                    $$content
