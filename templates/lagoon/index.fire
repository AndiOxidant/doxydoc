html
    head
        meta charset="UTF-8"
        script src="doxydoc.js"
        :each $customJS
            script src="$this"

        link rel="Stylesheet" href="main.css"
        link rel="stylesheet" href="highlight.css"
        :each $customCSS
            link rel="Stylesheet" href="$this"
    body
        section class="pageMain"
            (> header)
            section class="pageBody"
                aside class="pageLeft"
                    :each $navigationLinks : div class="pageLinks"
                        span class="link"
                            a href="$link" target="$target.ifFalse('_self')" $name
                section class="pageContent"
                    $content
