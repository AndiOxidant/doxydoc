html
    head
        meta charset="UTF-8"
        script src="${basePath}doxydoc.js"
        :each $customJS
            script src="$this"

        link rel="Stylesheet" href="${basePath}main.css"
        link rel="stylesheet" href="${basePath}highlight.css"
        :each $customCSS
            link rel="Stylesheet" href="$this"
    body
        section class="pageMain"
            (> header)
            section class="pageBody"
                aside class="pageLeft"
                    :each $navigationLinks : div class="pageLinks"
                        span class="link"
                            a href="$link" target="$target.ifFalse('_self')" $name
                article class="pageContent"
                    $$content

            footer class="pageFooter" "Documentation created bys".
                a href="http://doxydoc.com" target="_blank" "DoxyDoc"
