html
    head
        script type="text/javascript"
        link rel="Stylesheet" href="public/lib/dresscode/dresscode.css"
        link rel="Stylesheet" href="public/doxit.css"
    body
        section class="pageMain"
            header class="pageHeader"
                div class="name" $name
                div class="version" "v"
                    $version
            section class="pageBody"
                aside class="pageLeft"
                    :each $listing : div
                        h1 $name
                        :each $groups : div class="groupBlock $id"
                            h2 $name
                            :each $items : ul class="groupBlockItem"
                                li 
                                    a href="#$name" $name
                section class="pageContent"
                    h1 $name
                        span class="version" $version
                    div class="description" $description
                    :each $listing : div
                        h2  $name
                        :each $groups : div class="groupBlock $id"
                            h3 id="$name" $name
                            :each $items : div
                                h3 $name
                                div class="description"
                                    $description.full
                                :if $example : div class="example"
                                    div $example
                                :if $code : div class="codeBox"
                                    code class="code" $code

            footer class="pageFooter" "Documentation generated by Doxit"