header class="pageHeader"
    section class="headerContent"
        div class="name"
            a class="logoLink" href="./" $name
        div class="version" "v"
            $version
        navi class="mainNavi"
            :each $headerLinks : div class="links"
                a href="$link" $name
nav class="pageNavi"
    section class="naviContent"
        :each $navigationLinks : div class="pageLinks"
            span class="link"
                a href="$link" target="$target.ifFalse('_self')" $name
                