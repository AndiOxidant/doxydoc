section class="docsItem"
    h3 $data.title
    article $data.description
    :if $data.tags.params
        h4 "Params"
        :each $data.tags.params : table class="paramsTable"
            tr
                td class="label label-$type" $type
                td class="name" $name
                td class="flags" $flags
                td class="description" $description
