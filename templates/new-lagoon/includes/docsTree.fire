section class="docsTree"
    :tree $docs.items : section class="tree"
        :if $atype.eq('package')
            section class="packages"
                h1 "Package: $title"
                    article $description
                    div "Items: $items.length"
        :if $atype.eq('subpackage')
            section class="subpackages"
                h1 "Subpackage: $title"
                    article $description
                    div "Items: $items.length"
        :if $atype.eq('module')
            section class="modules"
                h1 "Module: $title"
                    article $description
                    div "Items: $items.length"
        :if $atype.eq('submodule')
            section class="submodules"
                h1 "Supmodule: $title"
                    article $description
                    div "Items: $items.length"
        :if $atype.eq('group')
            section class="groups"
                h1 "Group: $title"
                    article $description
                    div "Items: $items.length"
        :if $atype.eq('content')
            section class="contents"
                h1 "Content: $title"
                    article $description
                    
        &next $items : section class="$atype items"
