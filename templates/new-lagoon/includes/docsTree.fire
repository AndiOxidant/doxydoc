section class="docsContent"
    :tree $docs.items : section class="docsItem $atype"
        :if $atype.eq('package')
            section class="blockItem"
                div class="typeLabel" "Package"
                h1 "$aname"
                h2 "$$data.title"
                article "$$data.description"
        :if $atype.eq('subpackage')
            section class="blockItem"
                div class="typeLabel" "Subpackage"
                h1 "$aname"
                h2 "$$data.title"
                article "$$data.description"
        :if $atype.eq('module')
            section class="blockItem"
                div class="typeLabel" "Module"
                h1 "$aname"
                h2 "$$data.title"
                article "$$data.description"
        :if $atype.eq('submodule')
            section class="blockItem"
                div class="typeLabel" "Submodule"
                h1 "$aname"
                h2 "$$data.title"
                article "$$data.description"
        :if $atype.eq('group')
            section class="blockItem"
                div class="typeLabel" "$aname"
                h1 "$aname"
                :if $items
                    :each $items
                        >includes/docsItem
                    
        &next $items : section class="$atype items"
