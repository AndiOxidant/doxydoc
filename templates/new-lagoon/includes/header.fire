header class="pageHeader"
    :if $logoImage : div class="logoImage"
        a class="logoLink" href="$basePath"
            img class="logoImage" src="$logoImage"
    div class="name"
        a class="logoLink" href="$basePath" $name
    div class="version" "v"
        $version
    :if $subtitle : div class="subtitle"
        $subtitle
    navi class="mainNavi"
        :each $navigation : div class="links"
            a class="link $cssClass.or('')" href="$link" $name