section class="docs-navi"
    :tree $docs.items : ul class="tree $atype"
        :if $atype.eq('package')
            li class="item"
                a class="link package-link" href="#package-$aname" "$aname"
                &next $items : ul class="sub-tree $items.atype"
        :if $atype.eq('subpackage')
            li class="item"
                a class="link package-link" href="#subpacakge-$aname" "$aname"
                &next $items : ul class="sub-tree $items.atype"
        :if $atype.eq('module')
            li class="item"
                a class="link module-link" href="#module-$aname" "$aname"
                &next $items : ul class="sub-tree $items.atype"
        :if $atype.eq('submodule')
            li class="item"
                a class="link module-link" href="#submodule-$aname" "$aname"
                &next $items : ul class="sub-tree $items.atype"
        :if $atype.eq('group')
            li class="item"
                h3 class="group-title" "$aname"
                &next $items : ul class="sub-tree $items.atype"
        :if $atype.eq('content')
            li class="item"
                a class="link content-link" href="#$alink" "$aname"
