html
    head
        meta charset="UTF-8"
        script src="${basePath}doxydoc.js"
        :each $scripts
            script src="$this"

        link rel="Stylesheet" href="${basePath}doxydoc-less.css"
        link rel="Stylesheet" href="${basePath}doxydoc.css"
        :each $styles
            link rel="Stylesheet" href="$this"
    body
        section class="pageMain"
            (> includes/header)
            section class="pageBody"
                aside class="pageLeft"
                    :each $sidebar : nav class="sidebar-navi"
                        ul class="link-list"
                            li class="link-item"
                                a class="link $cssClass.or('')" href="$link" target="$target" $name

                                :if $subNavigation
                                    :tree $subNavigation : ul class="sub-link-list"
                                        li class="sub-link-item"
                                            a class="link $cssClass.or('')" href="$link" "$text"
                                            &next $subNavigation : ul class="sub-link-list"
                article class="pageContent"
                    $$content
            footer class="pageFooter" "Documentation created with".
                a href="http://doxydoc.com" target="_blank" "DoxyDoc"
