.docsItem
    .blockItem
        position relative
        background-color #eee
        border-radius 7px
        padding 10px
        margin-bottom 10px

    h1
        margin-top 0
        text-transform capitalize

    h2
        margin-top 0
        font-weight normal
        font-size 15px

    h3
        color #333
        font-size 21px
        display inline-block
        vertical-align middle

    h4
        color #000
        font-size 13px
        margin-top 20px
        
    h5
        color #333
        font-size 12px
        
.return-block
    h4
        display inline-block
    .desc
        display inline-block
        margin-left 10px
        
.fires-table
    h5
        margin 0
    
    
.fires-args-table
    counter-reset args
    font-size 14px
    
    td
        text-align left
        background-color inherit !important
        
    .arg
        display inline-block
        